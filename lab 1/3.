public class Main{
    public static void main(String[] args){
        int id;
        id = 224108110;
        int last_three = id % 1000;

        int last = last_three % 10;
        last_three = last_three / 10;
        int sec_last = last_three % 10;
        last_three = last_three / 10;
        int thr_last = last_three % 10;
        last_three = last_three / 10;
        System.out.println("Last Number " + last + " ");
        System.out.println("Secound Last " + sec_last + " ");
        System.out.println("Third Last " + thr_last + " ");

    }
}
