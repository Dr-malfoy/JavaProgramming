import java.text.DecimalFormat;

public class Main{
    public static void main(String[] args){
        int h = 10;
        int w = 5;
        int area = h*w;
        System.out.println("Rectengle area : " + area);

        int r = 5;
        double pie  = 3.1416;
        double perimeter = (pie * Math.pow(r,2));
        DecimalFormat df = new DecimalFormat("#.##");
        String perimete_1 = df.format(perimeter);
        System.out.println("Circle perimeter : "+perimete_1);

    }
}
